<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Red">#ffec1e24</Color>
    <SolidColorBrush x:Key="RedBrushKey" Color="{StaticResource Red}" />

    <Color x:Key="WindowBackground">#FFFFFFFF</Color>
    <SolidColorBrush x:Key="WindowBackgroundBrushKey" Color="{StaticResource WindowBackground}" />

    <Color x:Key="WindowBorder">#FF000000</Color>
    <SolidColorBrush x:Key="WindowBorderBrushKey" Color="{StaticResource WindowBorder}" />

    <Color x:Key="WindowTitleBarDropShadowLine">#FF222222</Color>
    <SolidColorBrush x:Key="WindowTitleBarDropShadowLineBrushKey" Color="{StaticResource WindowTitleBarDropShadowLine}" />
    
    <Color x:Key="WindowTitleBarDropShadow">#FFF8F8F8</Color>
    <SolidColorBrush x:Key="WindowTitleBarDropShadowBrushKey" Color="{StaticResource WindowTitleBarDropShadow}" />

    <Color x:Key="SideMenuBackground">#FF36393f</Color>
    <SolidColorBrush x:Key="SideMenuBackgroundBrushKey" Color="{StaticResource SideMenuBackground}" />
    
    <Color x:Key="TextNormal">#FF000000</Color>
    <SolidColorBrush x:Key="TextNormalBrushKey" Color="{StaticResource TextNormal}" />

</ResourceDictionary>